{
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
},
  "rules": {
    "prettier/prettier": "error",
    "indent": ["error", 2],
    "semi": "error",
    "no-console": 0
  }
}
